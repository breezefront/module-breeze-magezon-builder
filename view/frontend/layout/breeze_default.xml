<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="better_compatibility" xsi:type="array">
                    <item name="Magezon_Core" xsi:type="boolean">true</item>
                    <item name="Magezon_Builder" xsi:type="boolean">true</item>
                    <item name="Magezon_PageBuilder" xsi:type="boolean">true</item>
                </argument>
                <argument name="bundles" xsi:type="array">
                    <item name="dynamic" xsi:type="array">
                        <item name="Magezon_Builder/js/vimeo/player" xsi:type="array">
                            <item name="path" xsi:type="string">Magezon_Builder/js/vimeo/player</item>
                            <item name="global" xsi:type="string">Vimeo.Player</item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
